{
  "private": true,
  "scripts": {
    "bootstrap": "node ./scripts/bootstrap.js",
    "check-release": "cross-env CREATE_TEMPLATES_JSON=true alita build",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty  ./ && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "screenshot": "pro screenshot --mobile",
    "start": "alita dev",
    "build": "alita build",
    "test": "umi-test"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.{js,ts,tsx}": "npm run lint-staged:js"
  },
  "dependencies": {
    "alita": "^2.6.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "resolutions":{
    "underscore": "1.12.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.0.3",
    "@testing-library/react": "^10.4.7",
    "@types/classnames": "^2.2.7",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.0.27",
    "@types/numeral": "^0.0.28",
    "@types/react-dom": "^16.8.4",
    "@umijs/fabric": "^2.0.0",
    "@umijs/test": "^3.2.25",
    "@umijs/types": "^3.2.25",
    "@umijs/utils": "^3.2.25",
    "cross-env": "^7.0.0",
    "eslint": "^7.2.0",
    "husky": "^4.0.1",
    "lerna": "^3.22.1",
    "lint-staged": "^10.2.11",
    "mockjs": "^1.1.0",
    "prettier": "^2.0.2",
    "stylelint": "^13.0.0",
    "typescript": "^4.0.3",
    "umi-plugin-templates-devtool": "0.0.5",
    "yorkie": "^2.0.0"
  },
  "checkFiles": [
    "**/*.ts*",
    "**/*.less",
    "**/*.md"
  ]
}
